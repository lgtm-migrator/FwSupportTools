<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="UpdateDesc" targetNamespace="http://tempuri.org/UpdateDesc.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/UpdateDesc.xsd" xmlns:mstns="http://tempuri.org/UpdateDesc.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:complexType name="UpdateDesc">
		<xs:sequence>
			<xs:sequence>
				<xs:element name="file">
					<xs:complexType>
						<xs:attribute name="name" type="xs:string" />
						<xs:attribute name="once" type="xs:boolean" default="false" />
						<xs:attribute name="exitVS" type="xs:boolean" default="false" />
						<xs:attribute name="ignoreFirstTime" type="xs:boolean" default="false" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
</xs:schema>